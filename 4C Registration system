
#include<bits/stdc++.h>
using namespace std;
#define optimize() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define  endl '\n'
int main()
{
    optimize();

    map<string,int>cnt;
    int n;
    cin>>n;
    while(n--)
    {
       string s;
       cin>>s;
       if(cnt[s]==0)cout<<"OK"<<endl;
       else cout<<s<<cnt[s]<<endl;

       cnt[s]++;
    }

    return 0;
}

