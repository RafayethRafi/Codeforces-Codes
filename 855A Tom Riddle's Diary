
//BISMILLAHIR RAHMANIR RAHIM

#include<bits/stdc++.h>
using namespace std;
#define optimize() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define  endl '\n'
int main()
{
    optimize();

    int n;
    cin>>n;
    map<string,bool> vstd;
    while(n--)
    {

        string s;
        cin>>s;

        if(vstd[s])cout<<"YES"<<endl;
        else cout<<"NO"<<endl;

        vstd[s]= 1;

    }


    return 0;
}
