Â //https://codeforces.com/contest/1562/problem/B
 
 
#include<bits/stdc++.h>
using namespace std;
#define endl '\n'
#define optimize() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);


bool prime(int n)
{
    if(n==1) return 1;
    for(int i=2;i*i<=n;i++)
    {
        if(n%i==0)
            return 1;
    }
    return 0;
}

int main()
{
    optimize();
    int t,i,j;
    cin>>t;
    while(t--)
    {
        int n,f=0;
        cin>>n;
        string s;
        cin>>s;
        for(i=0;i<n;i++)
        {
            int x = s[i] - '0';
            if(prime(x))
            {
                cout<<"1"<<endl<<x<<endl;
                f=1;
                break;
            }
        }
        if(f==1) continue;

        

        for(i=0;i<n-1;i++)
        {
            for(j=i+1;j<n;j++)
            {
                int p = s[i] -'0';
                int q = s[j] - '0';
                int x = (p*10) + q;
                if(prime(x))
                {
                    cout<<"2"<<endl<<x<<endl;
                    f=1;
                    break;
                }


            }
            if(f==1) break;
        }
        if(f==1) continue;
        cout<<n<<endl<<s<<endl;
    }

    return 0;
}
