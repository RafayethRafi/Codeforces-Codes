#include<bits/stdc++.h>
using namespace std;
#define optimize() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define  endl '\n'
int main()
{
    optimize();
    int n,m,i,j;
    cin>>n>>m;
    bool x =1;

    for(i=1;i<=n;i++)
    {
        if(i%2)
        {
            for(j=1;j<=m;j++) cout<<"#";

            cout<<endl;
        }
        else if(i%4==0)
        {
            cout<<"#";
            for(j=2;j<=m;j++) cout<<".";
            cout<<endl;
        }
        else
        {
            for(j=1;j<m;j++) cout<<".";
            cout<<"#";

            cout<<endl;
        }
    }

    return 0;
}

