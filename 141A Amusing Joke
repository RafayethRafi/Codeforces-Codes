//BISMILLAHIR RAHMANIR RAHIM

#include<bits/stdc++.h>
using namespace std;
#define optimize() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define  endl '\n'
int main()
{
    optimize();
    string a,b,c;
    cin>>a>>b>>c;


    if(a.size()+b.size()<c.size())
    {
        cout<<"NO"<<endl;
            return 0;
    }
    map<char,int>m,x;
    for(auto u:a) m[u]++;
    for(auto u:b) m[u]++;

    for(auto u:c) x[u]++;
    for(auto u:m)
    {
        if(u.second!=x[u.first])
        {
            cout<<"NO"<<endl;
            return 0;
        }
    }

    cout<<"YES"<<endl;


    return 0;
}


