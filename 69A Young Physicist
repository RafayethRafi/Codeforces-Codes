#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,i;
    int suma=0,sumb=0,sumc=0;
    cin>>n;
    for(i=0;i<n;i++)
    {
int a,b,c;
        cin>>a>>b>>c;
        suma=suma+a;
        sumb=sumb+b;
        sumc=sumc+c;
    }
    if(suma==0&&sumb==0&&sumc==0)
        cout<<"YES"<<endl;
    else cout<<"NO"<<endl;
    return 0;
}
