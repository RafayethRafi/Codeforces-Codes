
//BISMILLAHIR RAHMANIR RAHIM

#include<bits/stdc++.h>
using namespace std;
#define optimize() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define  endl '\n'
int main()
{
    optimize();

    set<int>s;

    int n;
    cin>>n;

    while(n--)
    {
        int a;
        cin>>a;
        s.insert(a);
    }

    if(s.size()==1) cout<<"NO"<<endl;
    else
    {
        cout<<*(++s.begin())<<endl;
    }


    return 0;
}
