
//BISMILLAHIR RAHMANIR RAHIM

#include<bits/stdc++.h>
using namespace std;
#define optimize() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define  endl '\n'
int main()
{
    optimize();

    int n,i;
    cin>>n;

    int a[n+5];

    if(n%2==1)
    {
        cout<<"-1"<<endl;
    return 0;
    }
    for(i=1;i<=n;i++)
    {
        if(i%2==1)
            a[i] = i+1;
        else a[i] = i-1;
    }

    for(i=1;i<=n;i++)
    {
        cout<<a[i]<<" ";
    }



    return 0;
}

