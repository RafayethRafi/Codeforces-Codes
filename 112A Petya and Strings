#include<bits/stdc++.h>
using namespace std;
#define optimize() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define  endl '\n'
int main()
{
    optimize();
    string a,b;
    cin>>a>>b;
    int x=a.size();
    for(int i=0;i<x;i++)
    {
        a[i]= tolower(a[i]);
        b[i] = tolower(b[i]);
    }
    if(a>b)
        cout<<"1"<<endl;
    else if(b>a)
        cout<<"-1"<<endl;
    else cout<<"0"<<endl;
    return 0;
}

